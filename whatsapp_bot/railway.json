{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python ai_parser_standalone.py",
    "restartPolicyType": "NEVER"
  },
  "environments": {
    "production": {
      "variables": {
        "PYTHONPATH": "/app",
        "SERVICE_TYPE": "AI_PARSER_STANDALONE"
      }
    }
  }
}